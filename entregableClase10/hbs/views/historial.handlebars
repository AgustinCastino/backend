
    <h2>Historial</h2>
    {{#if productos}}
        <div class="table-responsive">
            <table class="table table-dark">
                <tr>
                    <th>Nombre</th>
                    <th>Precio</th>
                    <th>Foto</th>
                </tr>
                {{#each productos }}
                    <tr>
                        <td>
                            {{this.nombre}}
                        </td>
                        <td>
                            {{this.precio}}
                        </td>
                        <td>
                            {{this.foto}}
                        </td>
                    </tr>
                {{/each}}
                    
            </table>
        </div>
    {{else}}
        <h3 class="alert alert-danger">NO hay productos registrados</h1>
    {{/if}}

    <form action="/" method="get">
        <button class="btn btn-success">Agregar productos</button>
    </form>
